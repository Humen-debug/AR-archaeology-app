{
  "expo": {
    "name": "XR-archaeology",
    "slug": "XR-archaeology",
    "scheme": "apsapapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.apsap.arapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3c909800-1b86-47d8-86e5-52ac2a13842a"
      }
    },
    "owner": "catpotato"
  },
  "experiments": {
    "tsconfigPaths": true
  },
  "packagerOpts": {
    "config": "metro.config.js",
    "sourceExts": [
      "js",
      "jsx",
      "json",
      "wasm",
      "svg"
    ]
  }
}
